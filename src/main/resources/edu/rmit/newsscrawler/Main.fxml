<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="527.0" prefWidth="790.0"
            xmlns="http://javafx.com/javafx/11.0.14-internal"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="edu.rmit.newsscrawler.HelloController">
   <children>
      <Pane prefHeight="86.0" prefWidth="790.0">
         <children>
            <Label layoutX="14.0" layoutY="35.0" text="Categories:" />
         </children>
      </Pane>
      <ScrollPane layoutX="103.0" layoutY="14.0" prefHeight="55.0" prefWidth="675.0">
         <content>
            <HBox prefHeight="39.0" prefWidth="675.0">
               <children>
                  <ToggleButton maxHeight="100.0" mnemonicParsing="false" selected="true" text="HOME">
                     <toggleGroup>
                        <ToggleGroup fx:id="categories" />
                     </toggleGroup>
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin></ToggleButton>
                  <ToggleButton maxHeight="100.0" mnemonicParsing="false" text="ToggleButton" toggleGroup="$categories">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin></ToggleButton>
               </children>
            </HBox>
         </content>
      </ScrollPane>
      <ScrollPane layoutX="3.0" layoutY="86.0" prefHeight="443.0" prefWidth="790.0">
         <content>
            <VBox prefHeight="446.0" prefWidth="774.0">
               <children>
                  <Pane prefHeight="95.0" prefWidth="774.0" />
                  <ListView fx:id="topicList" prefHeight="274.0" prefWidth="774.0" />
               </children>
            </VBox>
         </content>
      </ScrollPane>
   </children>
</AnchorPane>
